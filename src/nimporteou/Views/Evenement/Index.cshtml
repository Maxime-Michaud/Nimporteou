@model nimporteou.Models.EvenementViewModels.ListeEvenementViewModel

@{
    ViewData["Title"] = "Index";
}

<h2>Liste d'événements</h2>

<div class="container">
    @foreach (var item in Model.Evenements)
    {
        Html.RenderPartial("_EvenementPartial", item);
    }       
</div>

<a asp-controller="Evenement" asp-action="Creer">Créer un nouvel évènement</a>